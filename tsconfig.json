{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"ignoreDeprecations": "6.0",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["./src/assets/*"],
			"@docs/*": ["./src/docs/*"],
			"@components/*": ["./src/components/*"],
			"@layouts/*": ["./src/layouts/*"]
		}
	}
}
